 then for l=28,90 do if 1~=d then do return e(1),e(4,t,f,n,e),e(5,t,f,n)end;break;end;do return function(d,e,n)if n then local e=(d/2^(e-1))%2^((n-1)-(e-1)+1);return e-e%1;else local e=2^(e-1);return(d%(e+e)&gt;=e)and 1 or 0;end;end;end;break;end;else do return e(1),e(4,t,f,n,e),e(5,t,f,n)end;end end else if d&gt;=6 then if 6&lt;d then if d&lt;8 then do return setmetatable({},{['__\99\97\108\108']=function(e,d,f,t,n)if n then return e[n]elseif t then return e else e[d]=f end end})end else do return n(d,nil,n);end end else do return t[n]end;end else if d&gt;2 then for l=19,80 do if 5~=d then local d=f;local h,f,l=t(2);do return function()local e,n,t,s=e(n,d(d,d),d(d,d)+3);d(4);return(s*h)+(t*f)+(n*l)+e;end;end;break;end;local d=f;do return function()local e=e(n,d(d,d),d(d,d));d(1);return e;end;end;break;end;else local d=f;do return function()local e=e(n,d(d,d),d(d,d));d(1);return e;end;end;end end end end),...)</textarea><div class="Box-sc-g0xbh4-0 cXpbTk"><div tabindex="0" class="Box-sc-g0xbh4-0 hHjsH"><div class="Box-sc-g0xbh4-0 GarDq react-code-file-contents" role="presentation" aria-hidden="true" data-tab-size="8" data-testid="code-lines-container" data-paste-markdown-skip="true" data-hpc="true" style="height: 20px;"><div class="react-line-numbers" style="pointer-events: auto; height: 20px; position: relative; z-index: 2;"><div data-line-number="1" class="react-line-number react-code-text" style="padding-right: 16px;">1</div></div><div class="react-code-lines" style="height: 20px;"><div data-key="0" class="react-code-text react-code-line-contents" style="min-height: auto;"><div><div id="LC1" class="react-file-line html-div" data-testid="code-cell" data-line-number="1" inert="inert" style="position: relative;">([[This file was protected with MoonSec V3]]):gsub('.+', (function(a) _mDQqdKLvVRSb = a; end)); cmDiMGRYxPXXmzWo=_ENV;BAGe_GwKVEeXAjo='w,&lt;JYniP_2k%ezvmx_,vkzJee%iJv2P_CP2Yz&lt;_i,kkk&lt;ie&lt;YizP2zJP%%Ykz2iikvYmvPiez_2&lt;&lt;,_2Y%en_z,mk%Jke2ni%YJvfP2Y&